{
   "background": {
      "scripts": [ "event.js", "global.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon128.png",
      "default_popup": "popup.html",
      "default_title": "Disable"
   },
   "content_security_policy": "script-src 'self' blob:; object-src 'self'",
   "description": "Allow the browser to play HLS video urls (m3u8) 'natively'",
   "icons": {
      "128": "img/icon128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Native HLS Playback",
   "omnibox": {
      "keyword": "m3u8"
   },
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "tabs", "*://*/*.m3u8*", "webRequest", "webRequestBlocking", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.12.4",
   "web_accessible_resources": [ "*.js", "*.html" ]
}
